	width: 100%;
	height: 100%;
	background-size: 100% 100%;
}
.header {
	background: #723473;
	color: white;
	padding: 20rpx;
	font-size: 40rpx;
	margin-bottom: 100rpx;
}
.qh-round {
	background: #9778a4;
	width: 160rpx;
	height: 160rpx;
	text-align: center;

	border-radius: 50%;
	display: -webkit-box;
