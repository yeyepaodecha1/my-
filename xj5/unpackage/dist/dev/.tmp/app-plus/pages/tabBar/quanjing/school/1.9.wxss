	display: -ms-flexbox;
	display: flex;
	margin: 50rpx auto;
}
navigator {
	text-decoration: none;
	font-size: 43rpx;
	line-height: 60rpx;
	color: white;
	-webkit-align-self: center;
	    -ms-flex-item-align: center;
	        align-self: center;
	text-align: center;
	margin: auto;
}

